<configuration>

   
    <appender name="out" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread]  %-5level [%line] %logger{36}.%M -- %msg%n</pattern>
        </encoder>
    </appender>


    <root level="info">
       
        <appender-ref ref="out"/>
     
    </root>

</configuration>